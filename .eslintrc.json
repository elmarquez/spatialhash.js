{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true
    },
    "extends": ["eslint:recommended", "plugin:jasmine/recommended"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [ "babel", "jasmine" ],
    "rules": {
        "babel/class-property": 0,
        "jasmine/new-line-before-expect": 0,
        "jasmine/new-line-between-declarations": 0,
        "no-unused-vars": "warn"
    }
}
